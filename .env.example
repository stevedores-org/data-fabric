# Data Fabric Environment Configuration
# Copy to .env and fill in your values

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# data-fabric Worker Configuration
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Local development URL (default: http://localhost:8787)
FABRIC_URL=http://localhost:8787

# Tenant configuration (for MCP server and testing)
TENANT_ID=test-tenant
TENANT_ROLE=admin

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# Gemini CLI Configuration
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Gemini API Key (get from https://aistudio.google.com/apikey)
# macOS: export GEMINI_API_KEY=$(security find-generic-password -s "GEMINI_API_KEY" -w)
# Linux/Windows: Set directly or use environment file
GOOGLE_API_KEY=your-gemini-api-key-here

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# Cloudflare Configuration (for production)
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Your Cloudflare account ID
# CLOUDFLARE_ACCOUNT_ID=your-account-id

# Cloudflare API token (for wrangler auth)
# CLOUDFLARE_API_TOKEN=your-api-token

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# Development Configuration
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Rust logging level (trace, debug, info, warn, error)
RUST_LOG=info

# Development mode
APP_ENV=development
